<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.wx.mvx"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.wx.mvx.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".mvc.controller.WeatherActivity"
            android:label="@string/mvc_name">
        </activity>
        <activity
            android:name=".mvp.view.WeatherActivity"
            android:label="@string/mvp_name"></activity>
        <activity
            android:name=".mvvm.view.WeatherActivity"
            android:label="@string/mvvm_name"></activity>
    </application>

</manifest>
